{
  "name": "keyv-s3",
  "description": "Amazon S3 storage adapter for Keyv",
  "homepage": "https://nicedoc.io/microlinkhq/keyv-s3",
  "version": "1.1.0",
  "main": "index.js",
  "author": {
    "email": "hello@microlink.io",
    "name": "microlink.io",
    "url": "https://microlink.io"
  },
  "contributors": [
    "Kiko Beats <josefrancisco.verdu@gmail.com>",
    "Keyvan Akbary <me@keyvanakbary.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/keyv-s3.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/keyv-s3/issues"
  },
  "keywords": [
    "amazon",
    "keyv",
    "s3"
  ],
  "dependencies": {
    "date-fns": "~2.16.1",
    "p-reflect": "~2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "aws-sdk": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "coveralls": "latest",
    "delay": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "got": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "nyc ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "ava": {
    "files": [
      "test/*.js",
      "!test/index.js"
    ],
    "serial": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard"
    ],
    "*.md": [
      "standard-markdown"
    ]
  }
}
